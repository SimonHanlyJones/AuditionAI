{
  "expo": {
    "name": "AuditionAI",
    "slug": "AuditionAI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#130526"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.simonhj.AuditionAI"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#130526"
      },
      "package": "com.simonhj.AuditionAI"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "22e77276-5012-410e-b7c1-3a2a5d211be7"
      }
    },
    "plugins": ["@react-native-voice/voice"]
  }
}
